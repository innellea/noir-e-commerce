{
    "name": "NOIR",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@sweetalert/with-react": "0.1.1",
        "@testing-library/jest-dom": "5.14.1",
        "@testing-library/react": "12.1.1",
        "@testing-library/user-event": "13.2.1",
        "bootstrap": "5.1.1",
        "env": "0.0.2",
        "firebase": "8.10.0",
        "gh-pages": "3.2.3",
        "prop-types": "15.7.2",
        "react": "^17.0.2",
        "react-awesome-slider": "^4.1.0",
        "react-bootstrap": "1.6.4",
        "react-dom": "^17.0.2",
        "react-redux": "7.2.5",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.3",
        "react-stripe-checkout": "2.6.3",
        "react-toggle-dark-mode": "1.0.4",
        "reactstrap": "8.10.0",
        "redux": "4.1.1",
        "redux-logger": "3.0.6",
        "redux-persist": "6.0.0",
        "redux-saga": "1.1.3",
        "redux-thunk": "2.3.0",
        "reselect": "4.0.0",
        "sass": "1.42.1",
        "snyk": "1.723.0",
        "styled-components": "5.3.1",
        "sweetalert": "2.1.2",
        "use-dark-mode": "2.3.1"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "test:staged": "cross-env CI=true react-scripts test --env=jsdom --findRelatedTests",
        "eject": "react-scripts eject"
    },
    "jest": {
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "collectCoverageFrom": [
            "src/**/*.js",
            "!src/index.js"
        ],
        "coverageReporters": [
            "text"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.js": [
            "eslint --fix",
            "npm run test:staged",
            "git add"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "cross-env": "7.0.3",
        "enzyme": "3.11.0",
        "enzyme-adapter-react-16": "1.15.6",
        "enzyme-to-json": "3.6.2",
        "husky": "7.0.2",
        "lint-staged": "11.1.2",
        "react-scripts": "^4.0.3"
    },
    "snyk": true
}
